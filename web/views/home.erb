<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
 <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>CoffeeW8 CoffeeMonitor</title>
    <link href="layout/styles.css" rel="stylesheet" type="text/css">
    <script language="javascript" type="text/javascript" src="js/jquery.js"></script>
    <script language="javascript" type="text/javascript" src="js/jquery.flot.js"></script>
 </head>
    <body>
    <h1>Coffee State</h1>
    <p id="info"></p>

    <div id="placeholder" style="width:900px;height:250px;"></div>

    <p id="info_date"></p>

    
    <p>
      <input id="refresh_rate_input" type="text" value="" style="text-align: right; width:5em"> 
      Time between updates in milliseconds (suggested 500-5000).
    </p>
    
    <p>
      <input id="data_size_input" type="text" value="" style="text-align: right; width:5em"> 
      Data size in entries (suggested 5-500).
    </p>
    
    <script src="js/coffeeplot.js" type="text/javascript"></script>

 </body>
</html>

